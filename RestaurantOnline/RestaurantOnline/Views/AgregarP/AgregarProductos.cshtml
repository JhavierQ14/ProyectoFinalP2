@model List<RestaurantOnline.Entidades.tbl_Producto>
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <title>Administrar Productos</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
</head>
<body>



    <h1 class="text-center"> Ingresar Productos </h1>
    <br />
    <div class="text-center">
        <table class="table table-hover">
            <thead>
            <th typeof="hidden">
                Id
            </th>
            <th>
                Nombre del producto
            </th>
            <th>
                Precio
            </th>
            <th>
                Fecha
            </th>
            <th>
                Estado Producto
            </th>
            <th>
                Imagen del producto
            </th>
            <th>
                Menú
            </th>
            <th>
                @Html.ActionLink("Agregar Productos", "DatosProductos", new { producto_id = 0 }, new { @class = "btn btn-success" })
            </th>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {

                    <tr>
                        <td>@item.producto_id</td>
                        <td>@item.nombreProducto</td>
                        <td>@item.precioP</td>
                        <td>@item.fechaCreacionP</td>
                        <td>@item.estadoProducto</td>
                        <td>@item.imageP</td>
                        <td>@item.menu_Fk</td>





                        @*<td>

                                @Html.ActionLink("Update", "Registrar", new { id = item.StudentId, apellido = item.LastName, nombre = item.FirstMiName, fecha = item.EnrollmentsDate }, new { @class = "btn btn-primary" })
                                @Html.ActionLink("Delete", "Delete", new { id = item.StudentId }, new { @class = "btn btn-danger" })

                            </td>*@
                    </tr>
                }
            </tbody>
        </table>
    </div>
</body>
</html>

